<h1>User#index</h1>
<p>Find me in app/views/user/index.html.erb</p>

<% @users.each do |user| %>
  <div>
    <% if user != current_user %>
      <%= user_gravatar user %>
      <%= content_tag(:p, "#{user.first_name} - #{user.last_name}") %>
      <%= render 'friendships/friendship' , user: user%>
    <% end %>
  </div>
<% end %>